{
  "name": "ebay-hunter-worker",
  "version": "1.0.0",
  "description": "Long-running worker for eBay deal hunting",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop ebay-hunter-worker",
    "pm2:restart": "pm2 restart ebay-hunter-worker",
    "pm2:logs": "pm2 logs ebay-hunter-worker",
    "pm2:status": "pm2 status",
    "cleanup:stale": "node cleanup-stale.js",
    "cleanup:stale:dry": "node cleanup-stale.js --dry-run",
    "cleanup:matches": "node clear-matches.js",
    "usage": "node usage.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.3.1"
  }
}
