# Supabase credentials (from your Supabase project settings)
SUPABASE_URL=https://hzinvalidlnlhindttbu.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Polling interval in seconds (default: 10)
POLL_INTERVAL=10

# Maximum tasks to process in parallel (default: 5)
# If you have 12 tasks and set this to 5:
#   - Batch 1: tasks 1-5 run together (staggered)
#   - Batch 2: tasks 6-10 run together (staggered)
#   - Batch 3: tasks 11-12 run together (staggered)
MAX_CONCURRENT_TASKS=5

# Delay in milliseconds between starting each task in a batch (default: 500)
# This prevents hitting eBay API with all requests at the exact same moment
# 500ms = 0.5 seconds between each task start
STAGGER_DELAY=500
